
<!-- Include Head -->
<%- include("./partials/head") %>
	<!-- Header -->

	<% if (locals.user != null) { %>
		<%- include("./partials/headerLogueado") %>
		<% } else { %>
			<%- include("./partials/header") %>
	<% } %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Resultados de b√∫squeda</h2>
				 <h2> <%=dic.mensaje  %></h2> 
			</div>
			<!-- Show the "ultimos" products -->
			<% for( let i = 0; i < dic.productos.length; i++ ) { %>
				<div class="col-12 col-sm-6 col-lg-3">
					<section class="product-box">
						<a href="/product/detail/<%= dic.productos[i].id %>">
							<figure class="product-box_image">
								<img src="<%= dic.productos[i].foto_producto %>" alt="botin2">
							</figure>
							<article class="product-box_data">
								<h2><%= dic.productos[i].nombre %></h2>
								<p><a href="/profile/id/<%= dic.productos[i].usuario.id %>"> By:<%= dic.productos[i].usuario.usuario %></a> </p>
								<p><%= dic.productos[i].descripcion %></p>
								<p>Comentarios: 12</p>
							</article>
						</a>
					</section>
				</div>
			<% } %>
			

	<!-- footer -->
	<%- include("./partials/footer") %>
	<!-- /footer -->
