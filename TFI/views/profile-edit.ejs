
<!-- Include Head -->
<%- include("./partials/head") %>
	<!-- Header -->

	<%- include("./partials/headerLogueado") %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 	
				<form action="/profile/profile-edit/<%= usuario.id %>" method="post"> 
					<div>
						<input type="email" name="email" value="<%= usuario.email %>" placeholder="Email" required>
					</div>
					<div>
						<input type="text" name="usuario" value="<%= usuario.usuario %>" placeholder="Usuario" required>
					</div>
					<div>
						<input type="password" name="clave" value="" placeholder="ContrseÃ±a" required>
					</div>
					<div>
						<input type="date" name="fecha_nacimiento" value="<%= usuario.fecha_nacimiento %>" placeholder="Fecha de nacimiento" required>
					</div>
					<div>
						<input type="number" name="dni" value="<%= usuario.dni %>" placeholder="Documento" required>
					</div>
					<div>
						<input type="text" name="foto_perfil" value="<%= usuario.foto_perfil %>" placeholder="Foto perfil" required>
					</div>
					<button type="submit">Enviar</button>				
				</form>
			</h2>


	<!-- footer -->
	<%- include("./partials/footer") %>
	<!-- /footer -->